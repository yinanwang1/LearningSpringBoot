<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--绑定接口-->
<mapper namespace="com.example.customer.mapper.AccountMapper">
    <select id="getAccount" resultType="com.example.customer.bean.Account">
        select *
        from a_account
        where id = #{id}
    </select>
</mapper>
